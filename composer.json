{
	"name": "werx/skeleton",
	"description": "Skeleton Application for the Werx Project",
	"homepage": "https://github.com/werx/skeleton",
	"license": "MIT",
	"authors": [
		{
			"name": "Josh Moody",
			"email": "josh@moody.io"
		}
	],
	"autoload": {
		"psr-4": {
			"werx\\Skeleton\\Tests\\": "tests/",
			"werx\\Skeleton\\Controllers\\": "src/controllers/",
			"werx\\Skeleton\\Entities\\": "src/entities/",
			"werx\\Skeleton\\Managers\\": "src/managers/",
			"werx\\Skeleton\\Models\\": "src/models/",
			"werx\\Skeleton\\Utils\\": "src/utils/",
			"werx\\Skeleton\\Validators\\": "src/validators/"
		}
	},
	"require": {
		"php": ">= 5.4",
		"werx/core": "1.*",
		"werx/validation": "1.*",
        "werx/forms": "1.*",
	  	"werx/messages": "1.*"
	},

	"require-dev": {
		"phpunit/phpunit": "~4.2",
		"squizlabs/php_codesniffer": "~2.2"
	},
	"minimum-stability": "dev"
}
